@article{jha_locally_2007,
	title = {A locally conservative finite element framework for the simulation of coupled flow and reservoir geomechanics},
	volume = {2},
	url = {https://www.semanticscholar.org/paper/211b8b11d83a5d35af6e730653928be21e27cba6},
