@article{molinariSelfConsistentApproach1987,
  title = {A Self Consistent Approach of the Large Deformation Polycrystal Viscoplasticity},
  author = {Molinari, A. and Canova, G. R. and Ahzi, S.},
  year = {1987},
  month = dec,
  journal = {Acta Metallurgica},
  volume = {35},
  number = {12},
  pages = {2983--2994},
  issn = {0001-6160},
  doi = {10.1016/0001-6160(87)90297-5},
  abstract = {A large deformation viscoplastic polycrystal theory is formulated. It is restricted to loading conditions since elasticity is entirely neglected. The single crystal behavior is based on crystallographic slip, and the slip rate is a non-linear function of the resolved shear stress. The time independent plasticity is obtained as a limit when the rate sensitivity tends to zero. The polycrystal behavior is obtained from the single crystal constitutive relation by solving integral equations derived from the field equations. A self-consistent approach is developed, where each grain is assumed to be a single ellipsoidal inclusion in a homogeneous equivalent medium. An interaction formula is derived and the local strain rates are calculated by resolving a nonlinear system of equations. Applications to texture predictions in tension, compression, rolling and torsion are finally considered. R\'esum\'e Nous formulons une th\'eorie des polycristaux viscoplastiques \`a forte d\'eformation, limit\'ee aux conditions de charge car on n\'eglige enti\`erement le domaine k\'elastique. Le comportement du monocristal est bas\'e sur le glissement cristallographique, et la vitesse de glissement est une fonction non lin\'eaire de la contrainte de cisaillement r\'eduite. La limite obtenue lorsque la sensibilit\'e \`a la vitesse tend vers z\'ero, constitue la plasticit\'e ind\'ependante du temps. On obtient le comportement du polycristal \`a partir de la relation de base du monocristal en r\'esolvant les \'equations int\'egrales d\'eriv\'ees des \'equations de champ. Nous d\'eveloppons une approche autocoh\'erente o\`u l'on assimile chaque grain \`a une inclusion ellipso\"idale unique dans un milieu homog\`ene \'equivalent. Nous en d\'eduisons une formule d'interaction et nous calculons les vitesses de d\'eformation locales en r\'esolvant un syst\`eme non lin\'eaire d'\'equations. Nous consid\'erons enfin les applications de cette approche \`a la pr\'evision des textures en tension, en compression, en laminage et en torsion. Zusammenfassung Es wird eine Theorie f\"ur das viskoplastische Verhalten von Polykristallen bei gro{$\beta$}en Verformungen vorgelegt. Diese Theorie ist auf Lastbedingungen beschr\"ankt, da die Elastizit\"at vollst\"angig vernachl\"assigt wird. Das Einkristallverhalten basiert auf kristallografischer Gleitung, die Gleitgeschwin-digkeitist eine nichtlineare Funktion der Flie{$\beta$}spannung. Das von der Zeit unabh\"angige plastische Verhalten ergibt sich, wenn die Geschwindigkeitsempfindlichkeit zu Null wird. Das Verhalten des Polykristalles ergibt sich aus den Einkristallbeziehungen, indem die aus den Feldgleichungen abgeleiteten Integralgleichungen gel\"ost werden. Eine selbst-konsistente N\"aherung wird entwickelt, bei dem jedes einzelne Korn als ein ellipsoidf\"ormiger Einschlu{$\beta$} in einem homogenen \"aquivalentmedium angesehen wird. Eine Wechselwirkungsformel wird hergeleitet und die lokalen Verformungsraten werden durch L\"osung eines nichtlinearen Gleichungssystems berechnet. Schlie{$\beta$}lich werden Anwendungen betrachtet, mit denen durch Zug, Druck, Wolzen und Torsion erzeugte Texturen vorausgesagt werden k\"onnen.},
  langid = {english},
  file = {/home/jose/Zotero/storage/ZXYQS97W/Molinari_Canova_Ahzi_1987_A self consistent approach of the large deformation polycrystal viscoplasticity.pdf}
}


@article{poulainFinitestrainElastoviscoplasticBehavior2014,
  title = {Finite-Strain Elasto-Viscoplastic Behavior of an Epoxy Resin: {{Experiments}} and Modeling in the Glassy Regime},
