@article{brusselle-dupendMechanicalBehaviorSemicrystalline2001,
  title = {Mechanical Behavior of a Semicrystalline Polymer before Necking. {{Part}} 1: {{Characterization}} of Uniaxial Behavior},
  shorttitle = {Mechanical Behavior of a Semicrystalline Polymer before Necking. {{Part}} 1},
