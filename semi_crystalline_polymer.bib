	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/nme.3001},
	keywords = {fluid{\textendash}structure interaction, algebraic multigrid, monolithic solution schemes},
	pages = {987--1016},
	file = {gee_et_al_2011_truly_monolithic_algebraic_multigrid_for_fluid{\textendash}structure_interaction.pdf:/home/jose/Zotero/storage/8U72GCWX/gee_et_al_2011_truly_monolithic_algebraic_multigrid_for_fluid{\textendash}structure_interaction.pdf:application/pdf;Snapshot:/home/jose/Zotero/storage/TEVXMSCN/nme.html:text/html},
