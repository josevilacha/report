	keywords = {Analog signal processing, Continuous linear time-invariant systems, Continuous-time signals, Distributional signals, Linear differential equations, Mathematical foundations},
	pages = {4465--4503},
	file = {Full Text PDF:/home/jose/Zotero/storage/UUTM4EQ3/Ciampa et al. - 2019 - Linear Differential Equations and Related Continuo.pdf:application/pdf;Linear Differential Equations and Related Continuous LTI Systems:/home/jose/Zotero/storage/LHFHLZKN/10.1007@s00034-019-01080-7.pdf.pdf:application/pdf},
