@article{matthies_algorithms_2006,
	series = {Fluid-{Structure} {Interaction}},
	title = {Algorithms for strong coupling procedures},
	volume = {195},
	issn = {0045-7825},
	url = {https://www.sciencedirect.com/science/article/pii/S0045782505001969},
	doi = {10.1016/j.cma.2004.11.032},
	abstract = {This paper considers algorithms for computing the response of a coupled problem with a partitioned approach. One important example treated here is fluid{\textendash}structure interaction (FSI). Often procedures or even software exists to solve each sub-problem separately, and one wants to couple both. This setting seems to allow only the so-called weak coupling which is not sufficient for some problems. The so-called strong coupling is often a totally implicit formulation, where the system components are evaluated at the same time level. This usually requires an iterative procedure. The FSI problem is cast as an abstract differential algebraic equation (DAE), for which the coupling procedures are developed. With the partitioned approach, one simple and frequently used computational procedure is similar to a block-Gauss{\textendash}Seidel iteration. It is shown why this approach may experience difficulties, and how they may be circumvented with Newton-like methods still staying in the partitioned framework. The functional and software engineering requirements for the simulation interface are described and analysed. Some simple coupled example problems demonstrate how the proposed procedures work.},
	language = {en},
	number = {17},
	urldate = {2021-10-13},
	journal = {Computer Methods in Applied Mechanics and Engineering},
	author = {Matthies, Hermann G. and Niekamp, Rainer and Steindorf, Jan},
	month = mar,
	year = {2006},
	keywords = {Fluid{\textendash}structure interaction, Block-Newton methods, Partitioned methods, Quasi-Newton methods, Strong coupling, classified, to read},
	pages = {2028--2049},
	file = {Matthies et al. - 2006 - Algorithms for strong coupling procedures.pdf:/home/jose/Zotero/storage/ZTYHJFVS/Matthies et al. - 2006 - Algorithms for strong coupling procedures.pdf:application/pdf;matthies_et_al_2006_algorithms_for_strong_coupling_procedures.pdf:/home/jose/Zotero/storage/IYM8AUZA/matthies2006.pdf:application/pdf},
}

