	pages = {3111--3126},
	file = {kl{\"o}ppel_et_al_2011_fluid{\textendash}structure_interaction_for_non-conforming_interfaces_based_on_a_dual.pdf:/home/jose/Zotero/storage/UJDAEMAW/kl{\"o}ppel_et_al_2011_fluid{\textendash}structure_interaction_for_non-conforming_interfaces_based_on_a_dual.pdf:application/pdf;ScienceDirect Snapshot:/home/jose/Zotero/storage/3LM4DI9Q/S0045782511002246.html:text/html},
